{
  "kind": "collectionType",
  "collectionName": "product_categories",
  "info": {
    "name": "ProductCategory",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "types": {
      "type": "json"
    },
    "category": {
      "model": "category",
      "via": "product_categories"
    },
    "products": {
      "collection": "product"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}
